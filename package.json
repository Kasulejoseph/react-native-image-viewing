{
  "name": "react-native-image-with-overlay-viewer",
  "version": "0.0.1",
  "description": "React Native modal component for viewing images as a sliding gallery",
  "main": "lib/commonjs/index.js",
  "types": "lib/typescript/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kasulejoseph/react-native-image-viewing.git"
  },
  "keywords": [
    "react",
    "react-native",
    "image",
    "gallery",
    "image-gallery",
    "image-viewer"
  ],
  "scripts": {
    "postversion": "yarn build",
    "prepare": "bob build"
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-native": ">=0.61.3"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "readme.md",
    "package.json"
  ],
  "author": "Joseph Kasule",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kasulejoseph/react-native-image-viewing/issues"
  },
  "homepage": "https://github.com/Kasulejoseph/react-native-image-viewing#readme",
  "devDependencies": {
    "@babel/runtime": "7.23.6",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.64.12",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.1.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "module": "lib/module/index.js",
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
